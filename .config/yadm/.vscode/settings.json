{
  "editor.fontFamily": "Delugia Book, Cascadia Code PL, CaskaydiaCove Nerd Font, Menlo, monospace",
  "editor.fontWeight": "400",
  "files.associations": {
    "_code": "shellscript",
    ".aliases": "properties",
    ".gitconfig*": "properties",
    ".inputrc": "shellscript",
    ".protocol": "shellscript",
    "bootstrap-*": "shellscript",
    "bootstrap": "shellscript",
    "config": "properties",
    "encrypt": "properties",
    "yb": "shellscript"
  },
  "files.exclude": {
    "repo.git": true,
    "files.gpg": true
  },
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "activityBar.background": "#92725299",
    "activityBar.activeBackground": "#92725299",
    "activityBar.activeBorder": "#85ba9f",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#85ba9f",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#92725299",
    "titleBar.inactiveBackground": "#92725299",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#92725299",
    "statusBarItem.hoverBackground": "#ac8b6b99",
    "statusBar.foreground": "#e7e7e7",
    "sash.hoverBorder": "#92725299",
    "statusBarItem.remoteBackground": "#92725299",
    "statusBarItem.remoteForeground": "#e7e7e7"
  },
  "peacock.color": "#92725299",
  "cSpell.words": [
    "keyfile",
    "shellenv",
  ],
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.enabled": false,
}
